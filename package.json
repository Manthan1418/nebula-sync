{
  "name": "nebula-sync",
  "version": "1.0.0",
  "type": "module",
  "description": "Group Music Listening Platform - Synchronized music playback across devices",
  "scripts": {
    "build": "cd FE && npm install && npm run build && cd .. && npm run copy-static",
    "copy-static": "node -e \"const fs=require('fs');const path=require('path');function copyDir(s,d){fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copyDir(sp,dp):fs.copyFileSync(sp,dp)})};copyDir('FE/dist','public')\"",
    "start": "node server.js"
  },
  "keywords": [
    "music",
    "sync",
    "realtime",
    "socket.io",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
